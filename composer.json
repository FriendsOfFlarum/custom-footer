{
    "name": "fof/custom-footer",
    "description": "Allows the Addition of a Custom Footer",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/FriendsOfFlarum/custom-footer/issues",
        "source": "https://github.com/FriendsOfFlarum/custom-footer",
        "forum": "https://discuss.flarum.org/d/17774"
    },
    "homepage": "https://friendsofflarum.org",
    "funding": [
        {
            "type": "website",
            "url": "https://opencollective.com/fof/donate"
        }
    ],
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "David Sevilla Mart√≠n",
            "email": "david.s@redevs.org",
            "role": "Developer"
        }
    ],
    "replace": {
        "davis/flarum-ext-customfooter": "*"
    },
    "autoload": {
        "psr-4": {
            "FoF\\CustomFooter\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "FoF Custom Footer",
            "category": "feature",
            "icon": {
                "name": "fas fa-columns",
                "backgroundColor": "#e74c3c",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/17774"
        }
    }
}
